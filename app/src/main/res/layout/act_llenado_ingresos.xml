<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CBE8BA"
    tools:context=".LlenadoIngresos">

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="43dp"
        android:layout_marginEnd="25dp"
        android:text="Cantidad:"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/txtCantidad"
        app:layout_constraintEnd_toStartOf="@+id/txtCantidad"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/txtConceptoa"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginTop="78dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="64dp"
        android:background="@color/white"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="@+id/txtCantidad"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView16"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txtCantidad"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="37dp"
        android:background="@color/white"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView1"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp"
        android:text="Fecha:"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/textView1"
        app:layout_constraintTop_toBottomOf="@+id/textView1" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="43dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="63dp"
        android:text="Concepto"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/txtConceptoa"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtQuincena"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Quincena"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="113dp"
        tools:layout_editor_absoluteY="286dp" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Categoria"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="350dp" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="171dp"
        android:layout_height="32dp"
        android:text="Comentarios:"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="514dp" />

    <ImageButton
        android:id="@+id/btnCancelar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/transparente"
        android:tint="@color/transparente"
        app:srcCompat="@drawable/icons8_cancelar_100"
        tools:layout_editor_absoluteX="332dp"
        tools:layout_editor_absoluteY="20dp" />

    <EditText
        android:id="@+id/txtComentarios"
        android:layout_width="341dp"
        android:layout_height="109dp"
        android:background="@color/white"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="554dp" />

    <ImageButton
        android:id="@+id/btnAgregarImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/transparente"
        app:srcCompat="@drawable/icons8_add_image_96"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="498dp" />

    <EditText
        android:id="@+id/txtFecha"
        android:layout_width="207dp"
        android:layout_height="41dp"
        android:background="@color/white"
        android:clickable="true"
        android:ems="10"
        android:focusable="false"
        android:inputType="date"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="395dp" />

    <Button
        android:id="@+id/btnAceptar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="163dp"
        android:layout_marginTop="672dp"
        android:layout_marginEnd="150dp"
        android:layout_marginBottom="11dp"
        android:backgroundTint="#4C7A34"
        android:text="Aceptar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>